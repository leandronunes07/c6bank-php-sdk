    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.40"
    },
    "autoload": {
        "psr-4": {
            "LeandroNunes\\C6Bank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeandroNunes\\C6Bank\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "minimum-stability": "stable"
}
