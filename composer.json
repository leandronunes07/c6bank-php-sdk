    "name": "leandronunes07/c6bank-php-sdk",
    "description": "A PHP SDK for C6 Bank API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leandro Oliveira Nunes",
            "email": "leandro@agenciataruga.com"
        },
        {
            "name": "AgÃªncia Taruga",
